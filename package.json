{
  "name": "web-template",
  "author": "Jake Motta",
  "description": "A (heavy) web template project with the basics already pre-built. The goal of this repository is to be easily forked and modified for quick app prototyping.",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.17.3",
    "auth0": "^2.37.0",
    "auth0-lock": "^11.31.1",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "react": "^17.0.2",
    "react-animated-css": "^1.2.1",
    "react-css-theme-switcher": "^0.3.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux-immutable-state-invariant": "^2.1.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "cross-env REACT_APP_STAGE=local react-scripts start",
    "start-dev": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "start-prod": "cross-env REACT_APP_STAGE=production react-scripts start",
    "build": "react-scripts build",
    "build-dev": "cross-env REACT_APP_STAGE=dev npm run build",
    "build-prod": "cross-env REACT_APP_STAGE=production npm run build",
    "themify": "npx gulp less",
    "prettier": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.5.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.47.0"
  }
}
